<?xml version="1.0"?>

<PropertyList>
	<path>meikou-triton-white.ac</path>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>25000</max-m>
	</animation>

  <animation> 
    <type>textranslate</type> 
    <object-name>poly.024</object-name> 
    <object-name>poly.025</object-name>
    <property>/sim/time/sun-angle-rad</property>
    <step> 1.57 </step>
    <factor>0.318471338</factor>
    <center>
     <x-m> 0 </x-m>
     <y-m> 0 </y-m>
     <z-m> 0 </z-m>
    </center>
    <axis> 
      <x> -1 </x> 
      <y>  0 </y> 
      <z>  0 </z> 
    </axis> 
  </animation>

  <animation>
    <type>material</type>
       <object-name>poly.024</object-name> 
    <object-name>poly.025</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission>
      <red>   1.0 </red>
      <green> 1.0 </green>
      <blue>  1.0 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
        <object-name>poly.024</object-name> 
    <object-name>poly.025</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

<!-- strobe -->
    <animation>
